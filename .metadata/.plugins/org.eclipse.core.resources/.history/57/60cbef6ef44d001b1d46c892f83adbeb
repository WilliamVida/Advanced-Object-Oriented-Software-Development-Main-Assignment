package ie.gmit.sw;

import java.util.ArrayList;
import java.util.List;

public class JARInformationList {

	private static JARInformationList clf = new JARInformationList();

	private JARInformationList() {

	}

	public static JARInformationList getInstance() {
		return clf;
	}

	public List<String> getCustomerList() {
		List<String> customers = new ArrayList<>();


		return customers;
	}
	
}
