package ie.gmit.sw;

import java.util.ArrayList;
import java.util.List;

/**
 * This class is for the database but it does function correctly.
 * 
 * @author William Vida
 */
public class JARInformationList {

	/**
	 * Declare an instance of the class.
	 */
	private static JARInformationList jrl = new JARInformationList();

	private JARInformationList() {

	}

	/**
	 * This method return a list of the JAR information.
	 * 
	 * @return
	 */
	public static JARInformationList getInstance() {
		return jrl;
	}

	/**
	 * This would method return the JAR information.
	 * 
	 * @return
	 */
	public List<String> getJARInformation() {
		List<String> metrics = new ArrayList<>();

		metrics.add("Add to list");

		return metrics;
	}

}
